/* 侧边栏 */
.sidebar-content {
  position: absolute;
  top: 0;
  bottom: 0;
  overflow: hidden;
}
@keyframes showLeftSidebar
{
from {left: -100%;}
to {left: 0;}
}
@keyframes hideLeftSidebar
{
  from {left:0;}
  to {left: -100%;}
}
@keyframes showRightSidebar
{
from {right: -100%;}
to {right: 0;}
}
@keyframes hideRightSidebar
{
  from {right:0;}
  to {right: -100%;}
}
