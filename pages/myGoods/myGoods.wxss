.specialPrice {
    padding: 0 30rpx;
}
.goodsList {
    padding: 20rpx;
    border-bottom: 1px solid #d8d8d8;
    display: -webkit-flex;
    display: flex;
}

.goodsList:last-child {
    border-bottom: none;
}

.goodsList .goodsPic {
    width: 180rpx;
    height: 180rpx;
    border: 1px solid #d8d8d8;
}

.goodsInfo {
    padding: 0 20rpx;
    position: relative;
    flex: 1;
}

.swa{
  width: 100%;
  height: 12px;
}
.goodsTitle {
    position: relative;
    float: left;
    font-size: 34rpx;
    font-weight: bold;
    font-family: '微软雅黑';
}
.del{
    position: relative;
    float: right;
}
.my_icon{
  font-size: 34rpx;
  color: #e4452a;
}

.price {
    width: 100%;
    display: -webkit-flex;
    display: flex;
    align-items: flex-end;
    margin-left: 0px;
}
.price .newPrice {
    font-size: 35rpx;
    color: #e4452a;
    margin-top: 5rpx;
    margin-left: 0px;
    margin-right: 10rpx;
}
.price .oldPrice {
    text-decoration: line-through;
    color: #888;
    font-size: 30rpx;
    margin-top: 10rpx;
    align-items: -webkit-flex-end;
}

.status{
  /* border: 1px #e4452a solid; */
   margin-left: 0px;
   font-size: 30rpx;
   font-family: '微软雅黑';
}

.goodsTime {
    position: absolute;
    bottom: 0;
    font-size: 30rpx;
    font-family: '微软雅黑';
}